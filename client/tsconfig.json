{
    "compilerOptions": {
      "lib": ["DOM", "DOM.Iterable", "ES2022"],
      "types": ["node", "vite/client"],
      "target": "ES2022",
      "module": "ES2022",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "rootDirs": [".", "./.react-router/types"],
      "esModuleInterop": true,
      "verbatimModuleSyntax": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true
    },
    "include": ["*/**/*"],
  }
